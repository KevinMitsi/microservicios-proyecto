# Environment variables for msvc-profiles

# MongoDB Configuration
MONGO_URL=mongodb://profilesuser:profilespass@db-profiles:27017/profilesdb?authSource=admin
DATABASE_NAME=profilesdb

# RabbitMQ Configuration
RABBITMQ_URL=amqp://admin:admin@rabbitmq:5672
RABBITMQ_EXCHANGE=microservices.events
RABBITMQ_QUEUE=profiles.queue

# JWT Configuration (debe coincidir con msvc-auth)
JWT_SECRET=adasasSecretKeyForJWTs1234567890
JWT_ALGORITHM=HS256
JWT_ISSUER=msvc-auth

# Application Configuration
APP_NAME=msvc-profiles
APP_VERSION=1.0.0
